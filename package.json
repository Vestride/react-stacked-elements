{
  "name": "react-stacked-elements",
  "version": "1.0.1",
  "description": "Toggle the visibility of elements without causing a layout shift",
  "source": "src/index.tsx",
  "main": "dist/stacked-elements.js",
  "module": "dist/stacked-elements.module.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/stacked-elements.umd.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "npm run build && npm run build --prefix docs",
    "predeploy": "npm run compile",
    "deploy": "gh-pages -d docs/build",
    "build": "microbundle --jsx React.createElement",
    "dev": "microbundle watch --jsx React.createElement",
    "test": "jest"
  },
  "keywords": [
    "react",
    "stack",
    "elements",
    "components"
  ],
  "author": "Glen Cheney",
  "license": "MIT",
  "homepage": "https://vestride.github.io/react-stacked-elements",
  "repository": {
    "type": "git",
    "url": "git://github.com/Vestride/react-stacked-elements.git"
  },
  "peerDependencies": {
    "react": "^16.8.6 || 17",
    "react-dom": "^16.8.6 || 17"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^14",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-jest": "^27.4.6",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "microbundle": "^0.14.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  }
}
